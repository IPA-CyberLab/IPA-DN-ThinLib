# Clean
clean:
	-rm -f $(OBJECTS_MAYAQUA)
	-rm -f $(OBJECTS_CEDAR)
	-rm -f object/Ham.o
	-rm -f code/Ham.a
	-rm -f bin/ham
	-rm -f object/vpnserver.o
	-rm -f code/vpnserver.a
	-rm -f bin/vpnserver
	-rm -f object/vpnbridge.o
	-rm -f code/vpnbridge.a
	-rm -f bin/vpnbridge
	-rm -f object/vpnclient.o
	-rm -f code/vpnclient.a
	-rm -f bin/vpnclient
	-rm -f object/vpncmd.o
	-rm -f code/vpncmd.a
	-rm -f bin/vpncmd

# Help Strings
help:
	@echo "make [DEBUG=YES]"

